#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int main(){
    int i=rand();
    int n;
    cout<<"Digite um numero=\n";
    cin>>n;
    cout<<i<<" comparando numeros...";
    if (n>=i)
    cout<<"\nSeu numero e equivalente ou superior ao da maquina";
    else
    cout<<"\nSeu numero e inferior ao da maquina";
}
